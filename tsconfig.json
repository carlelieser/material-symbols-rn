{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"jsx": "react-native",
		"lib": ["DOM", "ESNext"],
		"moduleResolution": "node",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"target": "ESNext",
		"declaration": true
	},
	"include": [
		"src/*"
	],
	"exclude": [
		"node_modules"
	]
}

{
	"name": "material-symbols-rn",
	"version": "1.0.0",
	"description": "React Native SVG components for Material Symbols.",
	"main": "dist/index.js",
	"scripts": {
		"clean": "rimraf ./dist",
		"format": "prettier ./scripts --write",
		"download": "node scripts/download.mjs",
		"svg-tsx": "node scripts/svg-tsx.mjs",
		"build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' tsc",
		"clean-build": "npm run clean && npm run build",
		"update": "npm run download && npm run svg-tsx && npm run clean-build"
	},
	"keywords": [
		"material-symbols",
		"material",
		"symbols",
		"react-native",
		"svg",
		"icons"
	],
	"repository": {
		"url": "https://github.com/carlelieser/material-symbols-rn"
	},
	"author": "Carlos E. Santos",
	"license": "MIT",
	"dependencies": {
		"react": "^18.2.0",
		"react-native-svg": "^15.9.0"
	},
	"devDependencies": {
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@svgr/plugin-prettier": "^8.1.0",
		"@svgr/plugin-svgo": "^8.1.0",
		"@types/react": "^18.3.12",
		"@types/react-native": "^0.72.8",
		"cross-env": "^7.0.3",
		"fast-glob": "^3.3.2",
		"fs-extra": "^11.2.0",
		"node-fetch": "^3.3.2",
		"number-to-words": "^1.2.4",
		"p-queue": "^8.0.1",
		"prettier": "^3.1.1",
		"typescript": "^5.3.3",
		"unzipper": "^0.10.14",
		"yargs": "^17.7.2"
	}
}
